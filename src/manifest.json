{
  "name": "trello-copy-title-extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "*://*/*"
  ],
  "content_scripts": [{
    "matches": ["http://trello.com/*", "https://trello.com/*"],
    "js": ["index.js"]
  }],
  "background": {
    "scripts": [ "background.js" ]
  }
}
